{
  "name": "detect-amd",
  "version": "1.0.0",
  "description": "detect amd support from JavaScript source code",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/mocha --require should --reporter dot --ui bdd $(find test -name \"*.spec.js\")",
    "watch-test": "node_modules/.bin/mocha -w --require should --reporter list --ui bdd $(find test -name \"*.spec.js\")"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rpflorence/detect-amd.git"
  },
  "keywords": [
    "amd"
  ],
  "author": "Ryan Florence",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rpflorence/detect-amd/issues"
  },
  "dependencies": {
    "esprima": "~1.0.4"
  },
  "devDependencies": {
    "mocha": "~1.12.1",
    "should": "~1.2.2"
  }
}
